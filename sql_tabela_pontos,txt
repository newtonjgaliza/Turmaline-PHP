CREATE TABLE pontos_receita_orcamentaria (
	id SERIAL KEY,
	managementunitname INT NOT NULL,
	managementunitid INT NOT NULL,
	budgetrevenuesource INT NOT NULL,
	budgetrevenuedescription INT NOT NULL,
	predictedamount INT NOT NULL,
	collectionamount INT NOT NULL,
	municipio_id INT NOT NULL,
	CONSTRANT fk_municipio
		FOREIGN KEY (municipio_id)
		REFERENCES Município (id)
		ON DELETE CASCADE
		ON UPDATE CASCADE
);

CREATE TABLE pontos_receita_extarorcamentaria (
	id SERIAL KEY,
	managementunitname INT NOT NULL,
	managementunitid INT NOT NULL,
	realizedamount INT NOT NULL,
	extrabudgetrevenuesource INT NOT NULL,
	extrabudgetrevenuedescription INT NOT NULL,
	extrabudgetrevenueid INT NOT NULL,
	nomenclature INT NOT NULL,
	extrabudgetrevenuehistory INT NOT NULL,
	municipio_id INT NOT NULL,
	CONSTRANT fk_municipio
		FOREIGN KEY (municipio_id)
		REFERENCES Município (id)
		ON DELETE CASCADE
		ON UPDATE CASCADE
);

